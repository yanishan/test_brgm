ry(point) NULL,&#xD;&#xA;&#x9;point_RGF93LAMB93 geometry(point) NULL,&#xD;&#xA;&#x9;polygone_RGF93LAMB93 geometry(multipolygon) NULL,&#xD;&#xA;&#x9;point_WGS84UTM20 geometry(point) NULL,&#xD;&#xA;&#x9;polygone_WGS84UTM20 geometry(multipolygon) NULL,&#xD;&#xA;&#x9;point_RGFG95UTM22 geometry(point) NULL,&#xD;&#xA;&#x9;polygone_RGFG95UTM22 geometry(multipolygon) NULL,&#xD;&#xA;&#x9;point_RGSPM06U21 geometry(point) NULL,&#xD;&#xA;&#x9;polygone_RGSPM06U21 geometry(multipolygon) NULL,&#xD;&#xA;&#x9;point_RGM04UTM38S geometry(point) NULL,&#xD;&#xA;&#x9;polygone_RGM04UTM38S geometry(multipolygon) NULL&#xD;&#xA;)&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;CREATE TABLE historique_modification&#xD;&#xA;(&#xD;&#xA;&#x9;id_historique_modification_site integer NOT NULL,    -- Identifiant de l'historique des actions réalisées sur la fiche&#xD;&#xA;&#x9;id_lex_table integer NOT NULL,    -- Identifiant de la table (site, instruction ou classification) à laquelle se rapporte l'historique de la modification&#xD;&#xA;&#x9;id_objet integer NOT NULL,    -- Identifiant de l'objet dans la table spécifiée (site, instruction ou classification) auquel se rapporte la modification&#xD;&#xA;&#x9;nom_utilisateur varchar(255) NOT NULL,    -- Nom de l'utilisateur à l'origine de l'action&#xD;&#xA;&#x9;email_utilisateur varchar(255) NOT NULL,    -- Email de l'utilisateur à l'origine de l'action&#xD;&#xA;&#x9;date_action_numerique date NULL,    -- Date de l'action réalisée&#xD;&#xA;&#x9;id_lex_action_numerique integer NULL,    -- Identifiant de l'action réalisée&#xD;&#xA;&#x9;commentaire varchar(1000) NULL    -- Commentaires sur l'action réalisée&#xD;&#xA;)&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;CREATE TABLE instruction&#xD;&#xA;(&#xD;&#xA;&#x9;id_instruction integer NOT NULL,    -- Identifiant de l'instruction&#xD;&#xA;&#x9;code_metier varchar(11) NOT NULL,    -- Code métier de l'instruction&#xD;&#xA;&#x9;id_liste_instruction integer NULL,    -- Identifiant de la liste à laquelle fait partie l'instruction&#xD;&#xA;&#x9;de