    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;&#xD;&#xA;/* Create Tables */&#xD;&#xA;&#xD;&#xA;CREATE TABLE accident&#xD;&#xA;(&#xD;&#xA;&#x9;id_accident integer NOT NULL,    -- Identifiant de l'accident&#xD;&#xA;&#x9;id_site integer NULL,    -- Identifiant du site associé, lorsque l'accident est lié à une activité industrielle, que l'accident soit sur site ou hors site&#xD;&#xA;&#x9;id_lex_type_accident integer NULL,    -- Identifiant du type d'accident&#xD;&#xA;&#x9;date date NULL    -- Date de l'accident&#xD;&#xA;)&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;CREATE TABLE action&#xD;&#xA;(&#xD;&#xA;&#x9;id_action integer NOT NULL,    -- Identifiant de l'action&#xD;&#xA;&#x9;date_debut date NOT NULL,    -- Date de début de l'action&#xD;&#xA;&#x9;date_fin date NULL,    -- Date de fin de l'action&#xD;&#xA;&#x9;id_lex_type_action integer NOT NULL,    -- Identifiant du type d'action&#xD;&#xA;&#x9;description varchar(2000) NOT NULL,    -- Description de l'action&#xD;&#xA;&#x9;id_instruction integer NOT NULL,    -- Identifiant de l'instruction dans le cadre de laquelle l'action est réalisée&#xD;&#xA;&#x9;id_lex_milieu integer NULL,    -- Identifiant du milieu sur lequel porte l'action&#xD;&#xA;&#x9;id_lex_enjeu integer NULL    -- Identifiant de l'enjeu à protéger&#xD;&#xA;)&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;CREATE TABLE activite&#xD;&#xA;(&#xD;&#xA;&#x9;id_activite integer NOT NULL,    -- Identifiant de l'activité&#xD;&#xA;&#x9;id_ref_code_naf integer NOT NULL,    -- Identifiant de la valeur de lexique des codes NAF&#xD;&#xA;&#x9;date_debut date NULL,    -- Date de début de l'activité&#xD;&#xA;&#x9;date_fin date NULL,    -- Date de fin de l'activité&#xD;&#xA;&#x9;id_site integer NOT NULL,    -- Identifiant du site auquel l'activité se rapporte&#xD;&#xA;&#x9;activite_principale boolean NOT NULL    -- Indique si l'activité correspond à l'activité principale du site (code APE)&#xD;&#xA;)&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;CREATE TABLE \&quot;analyse\&quot;&#xD;&#xA;(&#xD;&#xA;&